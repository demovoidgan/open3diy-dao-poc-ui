{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "npm start",
            "command": "cd ${workspaceFolder}/open3diy-dao-poc-ui/ && export BROWSER=none && npm start",
            "isBackground": true,
            "problemMatcher": {
                "owner": "custom",
                "pattern": {
                    "regexp": "^(.*)$"
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": "Compiling...",
                    "endsPattern": "Compiled successfully."
                }
            }
        }
    ]
}